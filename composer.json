{
  "name": "paksuco/settings",
  "description": "WIP - Laravel settings page using options",
  "keywords": [
    "laravel",
    "settings",
    "UI",
    "options"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Taha PAKSU",
      "email": "tpaksu@gmail.com",
      "homepage": "https://rettica.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^7.0|^8.0",
    "illuminate/config": "^7.0|^8.0",
    "illuminate/routing": "^7.0|^8.0",
    "livewire/livewire": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Paksuco\\Settings\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Paksuco\\Settings\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Paksuco\\Settings\\SettingsServiceProvider"
      ],
      "aliases": {
        "Settings": "Paksuco\\Settings\\Facade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
